  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.5",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^5.0.14",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
